mixin portfolio()
  +b.SECTION.portfolio(id="portfolio").wow.bounceIn(data-wow-duration="2s" data-wow-delay=".9s")
    +e.wrapper
      +e.H2.title Наші ремонтні роботи
      +e.slider-overlay
        +e.slider.swiper-container
          +e.slider-wrap.swiper-wrapper
            +e.slider-item.swiper-slide
              +e.slider-show
                +e.slidetabso
                +e.slider-tabs
                  +e.slider-tab(data-active-slide='1')
                    +e.IMG(src="images/port_slide.jpg")
                  +e.slider-tab(data-active-slide='2')
                    +e.IMG(src="images/port_slide2.jpg")
                  +e.slider-tab(data-active-slide='3')
                    +e.IMG(src="images/port_slide3.jpg")

                +e.slider-show-wrap(data-count="1")
                    +e.slider-slider
                      +e.IMG(src="images/port_slide-before.jpg").slider-show-img.slider-show-before
                      +e.IMG(src="images/port_slide.jpg").slider-show-img.slider-show-after
                +e.slider-show-wrap(data-count="2").activeTab
